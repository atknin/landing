{
  "version": 2,
  "name": "seedstorm",
  "builds": [{
    "src": "package.json",
    "use": "@now/static-build"
  }],
  "routes": [{
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/img/(.*)",
      "dest": "/img/$1"
    },
    {
      "src": "/service-worker.js",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/og.png",
      "dest": "/og.png"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/normalize.css",
      "dest": "/normalize.css"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
